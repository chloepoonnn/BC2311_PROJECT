{"ast":null,"code":"import \"./assets/tailwind.css\";\nimport { computed, ref, watchEffect } from \"vue\";\nimport axios from 'axios';\nexport default {\n  name: \"App\",\n  setup() {\n    const coins = ref([]);\n    const search = ref('');\n    const retrieveCoins = async () => {\n      try {\n        const response = await axios.get(\"http://localhost:8082/crypto/coingecko/api/v1/market\");\n        coins.value = response.data;\n      } catch (err) {\n        console.log(err);\n      }\n    };\n    setInterval(() => {\n      retrieveCoins();\n    }, 100000);\n    const tenCoins = computed(() => {\n      return coins.value.slice(0, 10);\n    });\n    const matchedNames = computed(() => {\n      return coins.value.filter(coin => coin.id.includes(search.value));\n    });\n    retrieveCoins();\n    return {\n      tenCoins,\n      matchedNames,\n      search\n    };\n  }\n};","map":{"version":3,"names":["computed","ref","watchEffect","axios","name","setup","coins","search","retrieveCoins","response","get","value","data","err","console","log","setInterval","tenCoins","slice","matchedNames","filter","coin","id","includes"],"sources":["/Users/oscarlo/github/bc2311_project/frontend-vue/src/components/App.vue"],"sourcesContent":["<template>\n  <div class=\"marquee w-full h-12 overflow-hidden position-relative\">\n    <ul class=\"marquee-content h-full flex\" ref=\"mq\">\n      <li v-for=\"coin in tenCoins\" :key=\"coin.name\" class=\"flex justify-center items-center flex-shrink-0 text-white transform scale-75 lg:scale-100\">\n        <div class=\"flex justify-between w-3/4\">\n          <div class=\"flex items-center\">\n            <img :src=\"coin.image\" alt=\"coin logo\" class=\"w-4 h-4 lg:w-6 lg:h-6 rounded-full mr-4 object-cover\" />\n            <div class=\"hidden lg:block\">\n              <p class=\"font-bold\">{{ coin.name }}</p>\n              <p class=\"text-xs uppercase tracking-widest\">{{ coin.symbol }}</p>\n            </div>\n          </div>\n          <div>\n            <p class=\"font-bold text-xs lg:text-base flex justify-end \">{{ $filters.comma_separator(coin.current_price) }}</p>\n            <p v-if=\"$filters.price_negative(coin.price_change_percentage_24h)\" class=\"font-bold text-xs text-red-400 flex justify-end items-center \">\n              <fa icon=\"caret-down\" class=\"mr-1\" />\n              {{ $filters.string_trunc(coin.price_change_percentage_24h, 5) }}%\n            </p>\n            <p v-else class=\"font-bold text-xs text-green-400 flex justify-end items-center \">\n              <fa icon=\"caret-up\" class=\"mr-1\" />\n              {{ $filters.string_trunc(coin.price_change_percentage_24h, 5) }}%\n            </p>\n          </div>\n        </div>\n      </li>\n    </ul>\n  </div>\n  <div class=\"container mx-auto pt-18 px-2\">\n    <table class=\"table-fixed cursor-pointer\">\n      <caption class=\"table-title font-bold text-gray-700  pb-2\">Cryptocurrency Exchange - Top 20 (Market Cap)</caption>\n      <thead>\n        <tr class=\"relative text-left text-gray-600 text-sm\">\n          <td class=\"p-2\">\n            Search:\n            <input type=\"text\" placeholder=\"Coin Name ...\" class=\"border border-gray-500 rounded p-2\" v-model=\"search\" />\n          </td>\n        </tr>\n        <tr class=\"text-left bg-gray-100 text-gray-600 text-sm\">\n          <th class=\"w-1/4 p-4\">Coin Name</th>\n          <th class=\"w-1/4\">Market Price(Real Time)</th>\n          <th class=\"w-1/4\">Change%(24 Hours)</th>\n          <th class=\"w-1/4 hidden sm:table-cell\">Trading Volume</th>\n          <th class=\"w-1/4 hidden sm:table-cell\">Market Capitalization</th>\n        </tr>\n      </thead>\n      <tbody class=\"divide-y\">\n        <tr v-for=\"coin in matchedNames\" :key=\"coin.name\" class=\"text-sm hover:bg-gray-100 transition duration-300\">\n          <td class=\"p-4 flex items-center\">\n            <p class=\"mr-2\">{{ coin.market_cap_rank }}</p>\n            <img :src=\"coin.image\" alt=\"coin logo\" class=\"w-7 h-7 rounded-full mr-1\" />\n            <p class=\"font-bold p-1 mr-1\">{{ coin.name }}</p>\n            <p class=\"uppercase text-gray-500 hidden sm:table-cell\">{{ coin.symbol }}</p>\n          </td>\n          <td class=\"font-bold text-gray-600\">\n            ${{ $filters.comma_separator(coin.current_price) }}\n          </td>\n          <td class=\" font-bold\">\n            <div v-if=\"$filters.price_negative(coin.price_change_percentage_24h)\" class=\"text-red-500\">\n              <fa icon=\"caret-down\" class=\"mr-1\" />{{ coin.price_change_percentage_24h }}%\n            </div>\n            <div v-else class=\"text-green-500\">\n              <fa icon=\"caret-up\" class=\"mr-1\" />{{ coin.price_change_percentage_24h }}%\n            </div>\n          </td>\n          <td class=\"hidden sm:table-cell\">\n            <p style=\"color:rgb(26, 137, 165)\">{{ $filters.comma_separator(coin.total_volume) }} </p>\n          </td>\n          <td class=\"pr-10 hidden sm:table-cell\">\n            <p style=\"color:rgb(26, 137, 165)\">${{ $filters.comma_separator(coin.market_cap) }} </p>\n          </td>\n        </tr>\n      </tbody>\n    </table>\n  </div>\n</template>\n\n<script>\nimport \"./assets/tailwind.css\";\nimport { computed, ref, watchEffect } from \"vue\";\nimport axios from 'axios'\n\nexport default {\n  name: \"App\",\n  setup() {\n    const coins = ref([]);\n    const search = ref('');\n\n    const retrieveCoins = async () => {\n      try {\n        const response = await axios.get(\"http://localhost:8082/crypto/coingecko/api/v1/market\");\n        coins.value = response.data;\n      } catch (err) {\n        console.log(err);\n      }\n    };\n\n    setInterval(() => {\n      retrieveCoins();\n    }, 100000);\n\n    const tenCoins = computed(() => {\n      return coins.value.slice(0, 10);\n    });\n\n    const matchedNames = computed(() => {\n      return coins.value.filter((coin) => coin.id.includes(search.value));\n    });\n\n    retrieveCoins();\n\n    return { tenCoins, matchedNames, search };\n  },\n};\n</script>\n\n<style>\n#app {\n  -webkit-font-smoothing: antialiased;\n  -moz-osx-font-smoothing: grayscale;\n}\n\n.marquee-content {\n  animation: scrolling 25s linear infinite;\n}\n\n.marquee-content li {\n  width: 19%;\n}\n\n.table-title {\n  font-size: 30px;\n  bottom: 30;\n  height: 1.5em;\n}\n\n.marquee:before,\n.marquee:after {\n  position: absolute;\n  top: 0;\n  width: 10rem;\n  height: 3em;\n  content: \"\";\n  z-index: 1;\n}\n\n.marquee {\n  background: rgb(6, 78, 107);\n}\n\n.marquee:before {\n  left: 0;\n  background: linear-gradient(to right, rgb(26, 137, 165) 0%, transparent 30%);\n}\n\n.marquee:after {\n  right: 0;\n  background: linear-gradient(to left, rgb(26, 137, 165) 0%, transparent 30%);\n}\n\n@keyframes scrolling {\n  0% {\n    transform: translateX(0);\n  }\n\n  100% {\n    transform: translateX(-100%);\n  }\n}\n</style>\n"],"mappings":"AA6EA,OAAO,uBAAuB;AAC9B,SAASA,QAAQ,EAAEC,GAAG,EAAEC,WAAU,QAAS,KAAK;AAChD,OAAOC,KAAI,MAAO,OAAM;AAExB,eAAe;EACbC,IAAI,EAAE,KAAK;EACXC,KAAKA,CAAA,EAAG;IACN,MAAMC,KAAI,GAAIL,GAAG,CAAC,EAAE,CAAC;IACrB,MAAMM,MAAK,GAAIN,GAAG,CAAC,EAAE,CAAC;IAEtB,MAAMO,aAAY,GAAI,MAAAA,CAAA,KAAY;MAChC,IAAI;QACF,MAAMC,QAAO,GAAI,MAAMN,KAAK,CAACO,GAAG,CAAC,sDAAsD,CAAC;QACxFJ,KAAK,CAACK,KAAI,GAAIF,QAAQ,CAACG,IAAI;MAC7B,EAAE,OAAOC,GAAG,EAAE;QACZC,OAAO,CAACC,GAAG,CAACF,GAAG,CAAC;MAClB;IACF,CAAC;IAEDG,WAAW,CAAC,MAAM;MAChBR,aAAa,CAAC,CAAC;IACjB,CAAC,EAAE,MAAM,CAAC;IAEV,MAAMS,QAAO,GAAIjB,QAAQ,CAAC,MAAM;MAC9B,OAAOM,KAAK,CAACK,KAAK,CAACO,KAAK,CAAC,CAAC,EAAE,EAAE,CAAC;IACjC,CAAC,CAAC;IAEF,MAAMC,YAAW,GAAInB,QAAQ,CAAC,MAAM;MAClC,OAAOM,KAAK,CAACK,KAAK,CAACS,MAAM,CAAEC,IAAI,IAAKA,IAAI,CAACC,EAAE,CAACC,QAAQ,CAAChB,MAAM,CAACI,KAAK,CAAC,CAAC;IACrE,CAAC,CAAC;IAEFH,aAAa,CAAC,CAAC;IAEf,OAAO;MAAES,QAAQ;MAAEE,YAAY;MAAEZ;IAAO,CAAC;EAC3C;AACF,CAAC"},"metadata":{},"sourceType":"module","externalDependencies":[]}